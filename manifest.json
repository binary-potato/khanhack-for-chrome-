// manifest.json
{
  "manifest_version": 3,
  "name": "KhanHack",
  "version": "5.7",
  "description": "A Chrome extension with khanhack functionality",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.khanacademy.org/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "hello.html"
  }
}
