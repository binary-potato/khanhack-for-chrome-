{
  "manifest_version": 3,
  "name": "KhanHack",
  "version": "5.7.1",
  "description": "Khan Academy Answer Hack",
  "permissions": [
    "activeTab",
    "*://*.khanacademy.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.khanacademy.org/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
